<h1>Modelos de Carros</h1>

<%= link_to 'Registrar novo modelo', new_car_model_path %>

<ul>
    <% @car_models.each do |model| %>
        <li>
            <%= link_to model.name, car_model_path(model) %>
            <%= model.year %>
            <%= model.fuel_type %>
            <%= model.motorization %>
            <%= model.manufacturer.name %>
            <%= model.car_category.name %>
        </li>
    <% end %>
</ul>